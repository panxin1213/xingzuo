button {
    border: 0;
}

.btn,button {
    margin: 0;
    padding: 0;
}

.btn {
    box-sizing: border-box;
    display: inline-flex;
    justify-content: center;
    align-items: center;
}

.btn.icon-share-1::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

.btn.icon-share-2::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

button::after {
    border: none;
}

button.btn-act {
    position: relative;
}

button.btn-act::after {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%!important;
    height: 100%!important;
    transform: none!important;
    background: rgba(0,0,0,.3);
    border: none;
    border-radius: inherit;
}

.nav-wrap {
    width: 100vw;
    height: auto;
    background: inherit;
}

.back {
    background: #6b3e90;
    border: none;
    padding: 0 6rpx;
    margin: 0!important;
    height: 52rpx;
    position: absolute;
    left: 30rpx;
    top: 50%;
    transform: translate3d(0,-50%,0);
    box-sizing: border-box;
    border-radius: 26rpx;
}

.back .txt {
    font-size: 28rpx;
    font-weight: 900;
    color: #fff;
    line-height: 1;
    padding-right: 6rpx;
    align-items: center;
    display: flex;
    justify-content: flex-start;
    height: 100%;
    width: 100%;
    background: none;
}

.back .txt::before {
    content: "";
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100%;
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    margin-right: 8rpx;
}

.back.back-act::after {
    content: "";
    width: 100%;
    height: 100%;
    border-radius: inherit;
    background: rgba(0,0,0,.3);
    position: absolute;
    top: 0;
    left: 0;
}

.icon-circle-close {
    background-size: 100%;
    background-position: 0 0;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    right: -60rpx;
    top: -60rpx;
}

.icon-circle-close-act {
    opacity: .6;
}

.icon-close {
    display: inline-block;
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: 0 0;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
}

.icon-close-act {
    opacity: .6;
}

.pub {
    width: 100vw;
    height: 100vh;
    background: rgba(0,0,0,.8);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99;
    display: flex;
    justify-content: center;
    align-items: center;
}

.wrap {
    width: 520rpx;
    height: auto;
    background: none;
    position: relative;
    animation: show .3s ease-in 0s forwards;
}

.wrap.hide {
    animation: hide .3s ease-out 0s forwards;
}

.inner {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 42rpx 0;
    background: #fff;
    border-radius: 24rpx;
}

.tit,.tit0 {
    font-weight: 700;
    color: #000;
    font-size: 30rpx;
    line-height: 1;
}

.tit i {
    color: #cd17cc;
    font-weight: 900;
    font-size: 50rpx;
    display: inline-block;
    line-height: 1;
}

.tit i::after,.tit i::before {
    content: "“";
    font-size: 36rpx;
    display: inline-block;
    color: #000;
    font-weight: 700;
    margin-top: -4rpx;
    line-height: 1;
}

.tit i::after {
    content: "”";
}

.tarot-tit {
    font-weight: 700;
    color: #000;
    font-size: 28rpx;
    line-height: 1;
    margin-bottom: 10rpx;
}

.tit-b {
    font-size: 22rpx;
    font-weight: 400;
    color: #666;
}

.tarot-tit1 {
    color: #000;
    font-size: 30rpx;
    line-height: 1;
    margin-bottom: 10rpx;
}

.tarot-tit1 i {
    font-weight: 700;
    font-size: 32rpx;
}

.tit0 {
    font-size: 36rpx;
}

.tit_b {
    color: #707070;
    font-size: 26rpx;
    font-weight: 500;
    line-height: 1;
    margin: 12rpx 0 49rpx;
}

.p {
    color: #333;
    font-weight: 600;
    font-size: 30rpx;
    line-height: 64rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.p i {
    background-image: url("data:image/png;base64,");
    width: 28rpx;
    height: 28rpx;
    margin-right: 11rpx;
    display: inline-block;
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: 100%;
}

.l-img {
    width: 210rpx;
    height: 152rpx;
    left: 0;
}

.l-img,.r-img {
    position: absolute;
    top: 0;
}

.r-img {
    width: 88rpx;
    height: 121rpx;
    right: 0;
}

.img {
    width: 403rpx;
    margin: 31rpx auto 0;
    border: none;
}

.img.border {
    border-top: 1rpx solid #b9b9b9;
    border-bottom: 1rpx solid #b9b9b9;
}

.s-img {
    width: 162rpx;
    height: 158rpx;
    margin-bottom: 9rpx;
}

.pub .btn {
    width: 270rpx!important;
    height: 80rpx!important;
    line-height: 80rpx!important;
    background: linear-gradient(90deg,#8f2df3,#e70ebc);
    font-size: 32rpx;
    font-weight: 700;
    color: #fff;
    padding: 0;
    margin: 34rpx auto 0;
    animation: pulse 1s infinite both;
    box-shadow: 0 10rpx 21rpx 0 rgba(4,0,0,.2);
}

.pub .btn,.pub .btn::after {
    border-radius: 40rpx;
}

.cancle {
    color: #707070;
    font-size: 28rpx;
    line-height: 1;
    font-weight: 500;
    padding-top: 26rpx;
}

.tarot-btn {
    display: flex;
    justify-content: center;
    align-items: center;
}

.tarot-btn i {
    display: inline-block;
    background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABUAAAASBAMAAABGPIgdAAAAFVBMVEUAAAD///////////////////////9Iz20EAAAABnRSTlMArI1wYVSAU7y7AAAAOUlEQVQI12NgNmBgABEg4JYIIUAgLE0AQgABU1oihAABNZCYGmkSBlACwUZTg9BLWBjhNjQ3I/sFABdgF9XaMPOHAAAAAElFTkSuQmCC")no-repeat 50%;
    background-size: 100%;
    width: 22rpx;
    height: 18rpx;
    margin-left: 12rpx;
}

@-webkit-keyframes show {
    from {
        opacity: 0;
        transform: translate3d(0,300px,0);
    }

    to {
        opacity: 1;
        transform: translate3d(0,0,0);
    }
}

@keyframes show {
    from {
        opacity: 0;
        transform: translate3d(0,300px,0);
    }

    to {
        opacity: 1;
        transform: translate3d(0,0,0);
    }
}

@-webkit-keyframes hide {
    from {
        opacity: 1;
        transform: translate3d(0,0,0);
    }

    to {
        opacity: 0;
        transform: translate3d(0,300px,0);
    }
}

@keyframes hide {
    from {
        opacity: 1;
        transform: translate3d(0,0,0);
    }

    to {
        opacity: 0;
        transform: translate3d(0,300px,0);
    }
}

@-webkit-keyframes pulse {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(1.05);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes pulse {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(1.05);
    }

    100% {
        transform: scale(1);
    }
}
    background: linear-gradient(180deg,rgba(255,240,247,.4),rgba(255,240,247,.6),rgba(255,240,247,.9) 40%,#fff0f7 60%);
}

.extend .block.white::before {
    background: linear-gradient(180deg,hsla(0,0%,100%,.4),hsla(0,0%,100%,.6),hsla(0,0%,100%,.9) 40%,#fff 60%);
}


button {
    border: 0;
}

.btn,button {
    margin: 0;
    padding: 0;
}

.btn {
    box-sizing: border-box;
    display: inline-flex;
    justify-content: center;
    align-items: center;
}

.btn.icon-share-1::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

.btn.icon-share-2::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

button::after {
    border: none;
}

button.btn-act {
    position: relative;
}

button.btn-act::after {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%!important;
    height: 100%!important;
    transform: none!important;
    background: rgba(0,0,0,.3);
    border: none;
    border-radius: inherit;
}

.nav-wrap {
    width: 100vw;
    height: auto;
    background: inherit;
}

.back {
    background: #6b3e90;
    border: none;
    padding: 0 6rpx;
    margin: 0!important;
    height: 52rpx;
    position: absolute;
    left: 30rpx;
    top: 50%;
    transform: translate3d(0,-50%,0);
    box-sizing: border-box;
    border-radius: 26rpx;
}

.back .txt {
    font-size: 28rpx;
    font-weight: 900;
    color: #fff;
    line-height: 1;
    padding-right: 6rpx;
    align-items: center;
    display: flex;
    justify-content: flex-start;
    height: 100%;
    width: 100%;
    background: none;
}

.back .txt::before {
    content: "";
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100%;
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    margin-right: 8rpx;
}

.back.back-act::after {
    content: "";
    width: 100%;
    height: 100%;
    border-radius: inherit;
    background: rgba(0,0,0,.3);
    position: absolute;
    top: 0;
    left: 0;
}

.icon-circle-close {
    background-size: 100%;
    background-position: 0 0;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    right: -60rpx;
    top: -60rpx;
}

.icon-circle-close-act {
    opacity: .6;
}

.icon-close {
    display: inline-block;
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: 0 0;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
}

.icon-close-act {
    opacity: .6;
}

.pop-box {
    width: 100vw;
    height: 100vh;
    background: rgba(0,0,0,.8);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99;
    display: flex;
    justify-content: center;
    align-items: center;
}

.container {
    width: 590rpx;
    height: auto;
    background: none;
    position: relative;
    animation: show .3s ease-in 0s forwards;
}

.container.hide {
    animation: hide .3s ease-out 0s forwards;
}

.content {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 0 0 50rpx;
    background: #fff;
    border-radius: 24rpx;
}

.title-box {
    width: 590rpx;
    height: 230rpx;
    position: relative;
}

.title-bg {
    width: 100%;
    height: 100%;
}

.sec-title {
    font-size: 26rpx;
    text-align: center;
    position: absolute;
    top: 110rpx;
    font-weight: 400;
    color: #9b0e93;
    letter-spacing: 10rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 100%;
}

.lucky_img {
    width: 100%;
    height: 400rpx;
    margin-top: -30rpx;
}

.title {
    width: 400rpx;
    height: 100rpx;
}

.btn {
    width: 400rpx!important;
    height: 90rpx;
    border-radius: 45rpx;
    line-height: 90rpx;
    text-align: center;
    padding: 0;
    margin: 0!important;
    color: #fff;
    font-weight: 800;
    font-size: 34rpx;
    box-sizing: border-box;
    display: inline-flex;
    z-index: 1;
}

.btn,.save-btn-box {
    justify-content: center;
    align-items: center;
}

.save-btn-box {
    background: none;
    margin-top: 50rpx!important;
    position: absolute;
    width: 590rpx!important;
    display: flex;
}

.save-btn {
    padding: 0;
    text-align: center;
    background: none;
    width: auto!important;
    border-bottom: .5px solid #fff;
    border-radius: 0;
    display: inline-flex;
    align-items: center;
    min-height: 0!important;
    height: auto!important;
}

.save-btn::before {
    content: "";
    width: 28rpx;
    height: 28rpx;
    display: inline-block;
    margin-right: 10rpx;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
    margin-top: -5rpx;
}

.save-btn-text {
    margin-bottom: 3rpx;
    font-size: 28rpx;
    font-weight: 500;
    color: #fff;
}

.save-btn-act {
    opacity: .7;
}

.share-btn {
    background: linear-gradient(158deg,#ff79ab,#ff49d4);
    box-shadow: 0 5px 8px 0 rgba(219,36,175,.2);
    margin-top: 10rpx!important;
}

.share-btn::before {
    content: "";
    width: 34rpx;
    height: 34rpx;
    display: inline-block;
    margin-right: 10rpx;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

button.btn-act::after {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%!important;
    height: 100%!important;
    transform: none!important;
    background: rgba(0,0,0,.3);
    border: none;
}

button.btn-act {
    position: relative;
}

.lucky-image-box {
    display: flex;
    justify-content: space-around;
    align-items: center;
    flex-wrap: wrap;
    margin: -30rpx 20rpx 0;
    z-index: 2;
}

.lucky-item {
    width: 33%;
    text-align: center;
    margin-bottom: 30rpx;
}

.lucky-image {
    width: 130rpx;
    height: 130rpx;
}

.lucky-name {
    font-weight: 800;
    color: #723d71;
    font-size: 30rpx;
}

.bottom-back {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
}
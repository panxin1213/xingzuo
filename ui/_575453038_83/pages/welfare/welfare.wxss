button {
    border: 0;
}

.btn,button {
    margin: 0;
    padding: 0;
}

.btn {
    box-sizing: border-box;
    display: inline-flex;
    justify-content: center;
    align-items: center;
}

.btn.icon-share-1::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

.btn.icon-share-2::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

button::after {
    border: none;
}

button.btn-act {
    position: relative;
}

button.btn-act::after {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%!important;
    height: 100%!important;
    transform: none!important;
    background: rgba(0,0,0,.3);
    border: none;
    border-radius: inherit;
}

.nav-wrap {
    width: 100vw;
    height: auto;
    background: inherit;
}

.back {
    background: #6b3e90;
    border: none;
    padding: 0 6rpx;
    margin: 0!important;
    height: 52rpx;
    position: absolute;
    left: 30rpx;
    top: 50%;
    transform: translate3d(0,-50%,0);
    box-sizing: border-box;
    border-radius: 26rpx;
}

.back .txt {
    font-size: 28rpx;
    font-weight: 900;
    color: #fff;
    line-height: 1;
    padding-right: 6rpx;
    align-items: center;
    display: flex;
    justify-content: flex-start;
    height: 100%;
    width: 100%;
    background: none;
}

.back .txt::before {
    content: "";
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100%;
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    margin-right: 8rpx;
}

.back.back-act::after {
    content: "";
    width: 100%;
    height: 100%;
    border-radius: inherit;
    background: rgba(0,0,0,.3);
    position: absolute;
    top: 0;
    left: 0;
}

.icon-circle-close {
    background-size: 100%;
    background-position: 0 0;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    right: -60rpx;
    top: -60rpx;
}

.icon-circle-close-act {
    opacity: .6;
}

.icon-close {
    display: inline-block;
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: 0 0;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
}

.icon-close-act {
    opacity: .6;
}

.act-end {
    text-align: center;
    color: #e5e5ee;
    line-height: 60rpx;
    font-size: 30rpx;
    padding-top: 50rpx;
}
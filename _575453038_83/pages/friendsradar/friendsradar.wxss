.container {
    background-image: url("data:image/png;base64,");
    background-size: 100% auto;
    background-position: 0 0;
    background-repeat: no-repeat;
    padding-bottom: 30rpx;
}

.share-btn-wrap {
    width: 100%;
    background: #222d4b;
    position: fixed;
    left: 0;
    bottom: 0;
    height: 140rpx;
    height: calc(140rpx + constant(safe-area-inset-bottom));
    height: calc(140rpx + env(safe-area-inset-bottom));
}

.share-btn-wrap,.share-btn-wrap .btn {
    box-sizing: border-box;
}

.share-btn-wrap .btn {
    width: 690rpx!important;
    height: 90rpx!important;
    padding: 0;
    margin: 21rpx auto auto;
    border-radius: 45rpx;
    background: linear-gradient(90deg,#12e55c,#08c827);
    display: flex;
    justify-content: center;
    align-items: center;
}

.share-btn-wrap .btn .btn-i {
    background-image: url(data:image/png;base64,);
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: 100%;
    width: 44rpx;
    height: 39rpx;
    margin-right: 21rpx;
    line-height: 1;
}

.share-btn-wrap .btn .btn-t {
    font-size: 34rpx;
    font-weight: 700;
    color: #fff;
    line-height: 1;
}

button {
    border: 0;
}

.btn,button {
    margin: 0;
    padding: 0;
}

.btn {
    box-sizing: border-box;
    display: inline-flex;
    justify-content: center;
    align-items: center;
}

.btn.icon-share-1::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

.btn.icon-share-2::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

button::after {
    border: none;
}

button.btn-act {
    position: relative;
}

button.btn-act::after {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%!important;
    height: 100%!important;
    transform: none!important;
    background: rgba(0,0,0,.3);
    border: none;
    border-radius: inherit;
}

.nav-wrap {
    width: 100vw;
    height: auto;
    background: inherit;
}

.back {
    background: #6b3e90;
    border: none;
    padding: 0 6rpx;
    margin: 0!important;
    height: 52rpx;
    position: absolute;
    left: 30rpx;
    top: 50%;
    transform: translate3d(0,-50%,0);
    box-sizing: border-box;
    border-radius: 26rpx;
}

.back .txt {
    font-size: 28rpx;
    font-weight: 900;
    color: #fff;
    line-height: 1;
    padding-right: 6rpx;
    align-items: center;
    display: flex;
    justify-content: flex-start;
    height: 100%;
    width: 100%;
    background: none;
}

.back .txt::before {
    content: "";
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100%;
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    margin-right: 8rpx;
}

.back.back-act::after {
    content: "";
    width: 100%;
    height: 100%;
    border-radius: inherit;
    background: rgba(0,0,0,.3);
    position: absolute;
    top: 0;
    left: 0;
}

.icon-circle-close {
    background-size: 100%;
    background-position: 0 0;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    right: -60rpx;
    top: -60rpx;
}

.icon-circle-close-act {
    opacity: .6;
}

.icon-close {
    display: inline-block;
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: 0 0;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
}

.icon-close-act {
    opacity: .6;
}

.container {
    flex-direction: column;
    justify-content: flex-start;
    overflow-x: hidden;
    padding-bottom: 0;
}

.container,.radar-wrap {
    display: flex;
    align-items: center;
}

.radar-wrap {
    justify-content: center;
    width: 666rpx;
    height: 666rpx;
}

.title {
    width: 652rpx;
    height: 190rpx;
    margin-bottom: 66rpx;
}

.tit-txt {
    font-size: 26rpx;
    font-style: italic;
    color: #c60cdc;
    margin-bottom: 80rpx;
    line-height: 1;
    margin-top: 9rpx;
}

.tips,.tit-txt {

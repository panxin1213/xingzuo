@-webkit-keyframes bgshow0 {
    0% {
        opacity: 0;
        transform: scale(0);
    }

    15% {
        opacity: 1;
        transform: scale(.5);
    }

    35% {
        opacity: 0;
        transform: scale(0);
    }

    36% {
        opacity: 0;
        transform: scale(0);
    }

    37% {
        opacity: 1;
        transform: scale(1.3);
    }

    100% {
        opacity: 0;
        transform: scale(0);
    }
}

@keyframes bgshow0 {
    0% {
        opacity: 0;
        transform: scale(0);
    }

    15% {
        opacity: 1;
        transform: scale(.5);
    }

    35% {
        opacity: 0;
        transform: scale(0);
    }

    36% {
        opacity: 0;
        transform: scale(0);
    }

    37% {
        opacity: 1;
        transform: scale(1.3);
    }

    100% {
        opacity: 0;
        transform: scale(0);
    }
}

@-webkit-keyframes bgshow1 {
    0% {
        opacity: 0;
        transform: scale(1);
    }

    15% {
        opacity: 1;
        transform: scale(1.5);
    }

    100% {
        opacity: 0;
        transform: scale(1);
    }
}

@keyframes bgshow1 {
    0% {
        opacity: 0;
        transform: scale(1);
    }

    15% {
        opacity: 1;
        transform: scale(1.5);
    }

    100% {
        opacity: 0;
        transform: scale(1);
    }
}
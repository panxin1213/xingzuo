}

.tips {
    font-size: 28rpx;
    color: hsla(0,0%,100%,.7);
    margin-top: 30rpx;
    margin-bottom: 22rpx;
}

.btn {
    width: 420rpx!important;
    height: 110rpx!important;
    background: linear-gradient(90deg,#e84aff,#ad15f4);
    color: #fff;
    font-weight: 700;
}

.btn,.btn::after {
    border-radius: 55rpx;
}

.btn.icon-share-2::before {
    width: 48rpx;
    height: 48rpx;
    margin-right: 18rpx;
}

.fast-wrap {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-bottom: 40rpx;
}

.fast-wrap .tit-img {
    width: 316rpx;
    height: 38rpx;
    margin-bottom: 15rpx;
    margin-top: 66rpx;
}

.fast-wrap .xz-wrap {
    font-size: 34rpx;
    font-weight: 400;
    color: #fff;
    margin-bottom: 20rpx;
    width: 268rpx;
    height: 48rpx;
    line-height: 48rpx;
    background: url("data:image/png;base64,")no-repeat 0 0;
    background-size: 100%;
    text-align: center;
}

.xz-list-wrap {
    width: 700rpx;
    height: 186rpx;
    background: rgba(62,46,102,.6);
    border-radius: 16rpx;
    padding: 6rpx;
    box-sizing: border-box;
    position: relative;
}

.none-fr,.xz-list-wrap .scroll-wrap {
    width: 690rpx;
    height: 174rpx;
    border-radius: 12rpx;
    border: 1px solid rgba(183,157,246,.2);
    box-sizing: border-box;
    white-space: nowrap;
}

.xz-list-wrap .scroll-wrap {
    padding-right: 90rpx;
}

.xz-list-wrap .scroll-wrap .scroll-box {
    width: 112rpx;
    text-align: center;
    display: inline-block;
    padding: 20rpx 0 0;
    box-sizing: border-box;
    margin: 0 6rpx;
}

.xz-list-wrap .scroll-wrap .scroll-box:first-child {
    margin-left: 16rpx;
    width: 112rpx;
}

.xz-list-wrap .scroll-wrap .scroll-box:last-child {
    margin-right: 20rpx;
}

.xz-list-wrap .scroll-wrap .scroll-box .box-img {
    width: 96rpx;
    height: 96rpx;
    border: 2rpx solid #ff5c92;
    border-radius: 50%;
    box-sizing: border-box;
    display: block;
    margin: auto;
}

.xz-list-wrap .scroll-wrap .scroll-box .box-img.b {
    border-color: #56ddff;
}

.xz-list-wrap .scroll-wrap .scroll-box .box-txt {
    width: 100%;
    font-size: 22rpx;
    font-weight: 400;
    color: #b79df6;
    display: block;
    margin-top: 10rpx;
    text-align: center;
    text-overflow: ellipsis;
    line-height: 1;
    white-space: nowrap;
    overflow: hidden;
}

.xz-list-wrap .shadow-r {
    width: 100rpx;
    height: 184rpx;
    background: linear-gradient(225deg,#753ab3,#593a99);
    box-shadow: -18rpx 0 18rpx 0 rgba(38,26,69,.8);
    border-radius: 0 16rpx 16rpx 0;
    border-left: none;
    right: 0;
    top: 0;
}

.xz-list-wrap .shadow-r,.xz-list-wrap .shadow-r::before {
    position: absolute;
    box-sizing: border-box;
}

.xz-list-wrap .shadow-r::before {
    content: "";
    width: 95rpx;
    height: 174rpx;
    top: 6rpx;
    left: 0;
    border: 1px solid rgba(183,157,246,.2);
    border-left: none;
    border-radius: 0 12rpx 12rpx 0;
}

.xz-list-wrap .shadow-r .total-fr {
    text-align: center;
    font-size: 24rpx;
    font-weight: 400;
    color: #fff;
    line-height: 30rpx;
    position: absolute;
    top: 50%;
    display: block;
    width: 100%;
    transform: translate3d(0,-50%,0);
}

.p {
    display: block;
}

.none-fr {
    text-align: center;
    color: #b69df6;
    display: flex;
    justify-content: center;
    align-items: center;

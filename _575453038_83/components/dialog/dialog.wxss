button {
    border: 0;
}

.btn,button {
    margin: 0;
    padding: 0;
}

.btn {
    box-sizing: border-box;
    display: inline-flex;
    justify-content: center;
    align-items: center;
}

.btn.icon-share-1::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

.btn.icon-share-2::before {
    content: "";
    display: inline-block;
    background: url("data:image/png;base64,")no-repeat 50%;
    background-size: 100%;
}

button::after {
    border: none;
}

button.btn-act {
    position: relative;
}

button.btn-act::after {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%!important;
    height: 100%!important;
    transform: none!important;
    background: rgba(0,0,0,.3);
    border: none;
    border-radius: inherit;
}

.nav-wrap {
    width: 100vw;
    height: auto;
    background: inherit;
}

.back {
    background: #6b3e90;
    border: none;
    padding: 0 6rpx;
    margin: 0!important;
    height: 52rpx;
    position: absolute;
    left: 30rpx;
    top: 50%;
    transform: translate3d(0,-50%,0);
    box-sizing: border-box;
    border-radius: 26rpx;
}

.back .txt {
    font-size: 28rpx;
    font-weight: 900;
    color: #fff;
    line-height: 1;
    padding-right: 6rpx;
    align-items: center;
    display: flex;
    justify-content: flex-start;
    height: 100%;
    width: 100%;
    background: none;
}

.back .txt::before {
    content: "";
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100%;
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    margin-right: 8rpx;
}

.back.back-act::after {
    content: "";
    width: 100%;
    height: 100%;
    border-radius: inherit;
    background: rgba(0,0,0,.3);
    position: absolute;
    top: 0;
    left: 0;
}

.icon-circle-close {
    background-size: 100%;
    background-position: 0 0;
    background-repeat: no-repeat;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    right: -60rpx;
    top: -60rpx;
}

.icon-circle-close-act {
    opacity: .6;
}

.icon-close {
    display: inline-block;
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: 0 0;
    background-image: url("data:image/png;base64,");
    width: 60rpx;
    height: 60rpx;
}

.icon-close-act {
    opacity: .6;
}

.dialog-wrap {
    width: 100vw;
    height: 100vh;
    background: rgba(0,0,0,.7);
    position: fixed;
    top: 0;
    left: 0;
    display: flex;
    justify-content: center;
    align-items: center;
}

.dialog-wrap .inner {
    width: 550rpx;
    border-radius: 24rpx;
    background: #fff;
    text-align: center;
    padding-top: 100rpx;
    padding-bottom: 60rpx;
    position: relative;
    box-sizing: border-box;
}

.dialog-wrap .tips {
    color: #666;
    font-size: 26rpx;
    line-height: 1;
    margin-bottom: 74rpx;
    font-weight: 400;
    display: inline-block;
}

.dialog-wrap .head-img {
    width: 170rpx;
    height: 170rpx;
    position: absolute;
    top: -95rpx;
    left: 50%;
    transform: translate3d(-50%,0,0);
    border-radius: 50%;
    overflow: hidden;
}

.form-item {
    display: flex;
    align-items: center;
    padding-left: 80rpx;
    margin-bottom: 52rpx;
}

.form-item .label {
    font-size: 32rpx;
    font-weight: 500;
    line-height: 1;
    color: rgba(0,0,0,.7);
}

.form-item .input {
    display: block;
    margin-left: 43rpx;
    color: #000;
    font-size: 32rpx;
    font-weight: 500;
}

.form-item .input.sex-input {
    display: flex;
    align-items: center;
}

.input .i {
    background: url("data:image/png;base64,") 50% no-repeat;
    background-size: 100%;
    display: inline-block;
    width: 26rpx;
    height: 26rpx;
    margin-left: 23rpx;
}

.input .i.groupstats {
    background-image: url("data:image/png;base64,");
}

.picker {
    display: flex;
    align-items: center;
    border-bottom: 1rpx solid #dedede;
    line-height: 1;
    padding-bottom: 9rpx;
    color: #000;
}

.btn,.picker {
    font-size: 32rpx;
    font-weight: 500;
}

.btn {
    background: linear-gradient(90deg,#e84aff,#ad15f4);
    border-radius: 45rpx;
    width: 400rpx!important;
    height: 90rpx!important;
    color: #fff;
    text-align: center;
    padding: 0!important;
    margin: auto!important;
    line-height: 90rpx;
}

.btn.groupstats {
    background: linear-gradient(90deg,#ff5c92,#ff2c8f);
}

.input .radio {
    display: flex;
    align-items: center;
    line-height: 1;
}

.input .radio .icon {
    width: 32rpx;
    height: 32rpx;
    display: inline-block;
    margin-right: 10rpx;
    border-radius: 50%;
    background: url("data:image/png;base64,")50% no-repeat;
    background-size: 100%;
}

.input .radio .icon.act {
    background: url("data:image/png;base64,")50% no-repeat;
    background-size: 100%;
    border: none;
}

.input .radio .icon.act.groupstats {
    background-image: url("data:image/png;base64,");
}

.input .radio .txt {
    color: #000;
    font-size: 32rpx;
    font-weight: 500;
}

.inner.tips-inner {
    padding: 0 0 58rpx;
}

.tips-inner .tips-top-bg {
    width: 100%;
    height: 230rpx;
}

.tips-inner .tips-tip {
    font-size: 26rpx;
    font-weight: 500;
    color: #707070;
    opacity: .8;
    padding-bottom: 23rpx;
    padding-top: 20rpx;
    line-height: 1;
}

.tips-inner .tips-con {
    font-size: 28rpx;
    font-weight: 500;
    color: #333;
    line-height: 64rpx;
    text-align: left;
    display: flex;
    align-items: center;
    padding-left: 47rpx;
}

.tips-inner .tips-con::before {
    content: "";
    background-image: url("data:image/png;base64,");
    width: 28rpx;
    height: 28rpx;
    margin-right: 11rpx;
    display: inline-block;
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: 100%;
}

.tips-inner .tips-btn {
    background: linear-gradient(-90deg,#8f2df3,#e70ebc);
    box-shadow: 0 10px 21px 0 rgba(4,0,0,.2);
    width: 310rpx!important;
}

.tips-inner .tips-con .s-tip {
    font-size: 22rpx;
    font-weight: 400;
    color: #aaa;
    vertical-align: middle;
    padding-left: 6rpx;
}
.star {
    width: 26rpx;
    height: 26rpx;
    background-image: url(data:image/png;base64,);
    background-size: 158rpx 67rpx;
    background-repeat: no-repeat;
    display: inline-block;
}

.star:not(:last-child) {
    margin-right: 6rpx;
}

.star.red {
    background-position: 0 -42rpx;
}

.star.blue {
    background-position: -66rpx -42rpx;
}

.star.orange {
    background-position: -34rpx -42rpx;
}

.star.green {
    background-position: -130rpx -42rpx;
}

.star-b {
    width: 38rpx;
    height: 38rpx;
}

.star-b.red {
    background-position: 0 0;
}

.star.default {
    background-position: -98rpx -42rpx;
}

.star-b.default {
    background-position: -46rpx 0;
}
movable-view {
    pointer-events: auto;
}

movable-area {
    pointer-events: none;
}

.bigMoveBtn {
    width: 160rpx;
    height: 160rpx;
}

.bigMoveBtn,.movebtn {
    position: fixed!important;
    top: 0;
    left: 0;
    z-index: 20;
}

.movebtn {
    width: 130rpx;
    height: 130rpx;
}

.share-btn {
    background: no-repeat 50%/100% url("data:image/png;base64,");
    margin: 0;
    padding: 0;
    border: none;
    width: 130rpx!important;
    height: 130rpx!important;
}

.share-btn-act {
    background: no-repeat 50%/100% url("data:image/png;base64,");
}

.pub-btn {
    background: no-repeat 50%/100% url("data:image/png;base64,");
    margin: 0;
    padding: 0;
    border: none;
    width: 130rpx!important;
    height: 130rpx!important;
}

.pub-btn-act {
    background: no-repeat 50%/100% url("data:image/png;base64,");
}

.lucky-btn {
    background: none;
    margin: 0;
    padding: 0;
    border: none;
    width: 160rpx!important;
    height: 280rpx!important;
    right: -12rpx;
}

.lucky-btn,.reddot {
    transition: right .8s ease-in-out;
}

.reddot {
    width: 26rpx;
    height: 26rpx;
    background: red;
    border-radius: 50%;
    position: absolute;
    top: 70rpx;
    right: 24rpx;
    z-index: 10;
    box-sizing: border-box;
    color: #fff;
    font-weight: 900;
    line-height: 26rpx;
    font-size: 18rpx;
    text-align: center;
}

.lucky-btn-act {
    opacity: .5;
}

.lucky-img {
    width: 160rpx;
    height: 280rpx;
}

.btn-act {
    position: relative;
}

.moveable-wrap {
    position: absolute;
}

.mentalTestBtn {
    margin: 0;
    padding: 0;
    border: none;
    width: 130rpx!important;
    height: 130rpx!important;
    background: none;
}

.mentalTestBtnAct {
    opacity: .7;
}

.testImage {
    width: 130rpx;
    height: 130rpx;
}
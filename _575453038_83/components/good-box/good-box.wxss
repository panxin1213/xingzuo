.goods-box {
    width: 100%;
    height: auto;
}

.goods-box .title {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-position: 0 0;
    background-size: 100%;
    width: 100%;
    height: 110rpx;
    line-height: 110rpx;
    margin-top: 60rpx;
    justify-content: space-between;
    padding: 0 25rpx 20rpx;
    box-sizing: border-box;
}

.goods-box .title,.goods-box .title .left {
    display: flex;
    align-items: center;
}

.goods-box .title .left {
    height: 100%;
}

.goods-box .title .fire-icon {
    width: 40rpx;
    height: 40rpx;
    margin-right: 12rpx;
}

.goods-box .title .main-title {
    font-size: 36rpx;
    font-weight: 700;
    color: #fff;
}

.goods-box .title .main-title::after {
    content: "";
    display: inline-block;
    width: 1px;
    height: 28rpx;
    margin: 0 15rpx;
    background: #fff;
    opacity: .4;
}

.goods-box .title .sub-title {
    font-size: 28rpx;
    font-weight: 300;
    color: #fff;
}

.goods-box .right .more-btn {
    width: 145rpx!important;
    height: 45rpx!important;
    line-height: 45rpx!important;
    background: #fff;
    border-radius: 22rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #f9316b;
}

.goods-box .more-icon {
    width: 10rpx;
    height: auto;
    margin-left: 6rpx;
}

.goods-list {
    background: #f8f2f2;
    border-radius: 20rpx 20rpx 0 0;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 0 30rpx 40rpx;
    margin-top: -20rpx;
}

.goods {
    width: 335rpx;
    background: #fff;
    border-radius: 20rpx;
    margin-top: 25rpx;
}

.goods-img {
    width: 100%;
    border-radius: 20rpx 20rpx 0 0;
}

.goods-info {
    padding: 20rpx 20rpx 30rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    height: 175rpx;
    box-sizing: border-box;
}

.goods-info .name {
    font-size: 28rpx;
    font-weight: 400;
    color: #000;
    line-height: 34rpx;
}

.goods-info .price {
    font-size: 34rpx;
    font-weight: 500;
    color: #fb2e2e;
    line-height: 36rpx;
}

.goods-info .price .rmb {
    font-size: 24rpx;
}

.goods-info .bottom {
    justify-content: space-between;
}

.goods-info .bottom,.goods-info .tag-list {
    display: flex;
    align-items: center;
}

.goods-info .tag-list {
    justify-content: flex-end;
}

.goods-info .tag {
    width: 86rpx;
    height: 36rpx;
    border-radius: 6rpx;
    font-size: 22rpx;
    font-weight: 500;
    line-height: 36rpx;
    text-align: center;
    margin-left: 10rpx;
}

.goods-info .bottom .healthy {
    background: #d6fced;
    color: #00b66b;
}

.goods-info .bottom .love {
    background: #ffeae7;
    color: #ff3008;
}

.goods-info .bottom .career {
    background: #deeaff;
    color: #1e82fe;
}

.goods-info .bottom .rich {
    background: #fff3da;
    color: #ff6a07;
}

.goods-list .more-box {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 40rpx;
}

.goods-list .more {
    font-size: 24rpx;
    font-weight: 400;
    color: #f9316b;
}

.goods-list .more-act {
    opacity: .6;
}